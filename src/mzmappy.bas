/*    char  y         BG面のY座標(0〜63)
/*    str   value     表示する文字列
/* out:
/*    なし
/*
func bg_print(x;char, y;char, value;str)
  int p = 0
  char data
  char v
  while asc(mid$(value, p+1, 1)) <> 0
    data = 0
    v = asc(mid$(value, p+1, 1))
    /* 数値
    if (v >= '0' and v <= '9') then {
      data = v - 16
    } else if (v >= 'A' and v <= 'Z') then {
      data = v - 64
    } else if (v = '-') then {
      data = 42
    } else if (v = '.') then {
      data = 46
    } else if (v = '!') then {
      data = 97
    } else if (v = '(') then {
      data = 104
    } else if (v = ')') then {
      data = 105
    }
    bg_put(0, x + p, y, pat_dat(0, 0, 1, data))
    p = p + 1
  endwhile
endfunc
