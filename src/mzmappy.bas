  s = right$("      " + str$(hiscore) , 6)
  bg_print(13, 1, s)
endfunc
/*
/* パターンデータ取得
/* in:
/*    char  vr        垂直反転(0:通常、1:垂直反転)
/*    char  hr        水平反転(0:通常、1:水平反転)
/*    char  pb        パレットブロック(1〜15)
/*    char  cd        パターンコード(0〜255)
/* out:
/*    int   パターンデータ
/*
func int pat_dat(vr;char, hr;char, pb;char, cd;char)
  return(vr*32768 + hr*16384 + pb*256 + cd)
endfunc
