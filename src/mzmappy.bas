func get_control()
  stk = stick(1)
  trg = 0
  if ((strig(1) = 1) and (trg_bk = 0)) then {
    trg = 1
    trg_bk = 1
  } else if ((strig(1) = 0) and (trg_bk = 1)) then {
    trg = 0
    trg_bk = 0
  }
  if (inkey$(0) = chr$(27)) then {
      game_status = C_GAME_STATUS_QUIT
  }
endfunc
