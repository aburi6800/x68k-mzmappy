func game_opening()
  str s
  erase_all()
  bg_set(0, 0, 0)
  bg_set(1, 1, 0)
  for i =0 to 31
    bg_put(1, i, 30, pat_dat(0, 0, 1, &H4A))
    bg_put(1, i, 31, pat_dat(0, 0, 1, &H4A))
  next
  s = "NOW ON GO TO THE STORY !"
  bg_print(4, 9, s)
  bg_set(0, 0, 1)
  bg_set(1, 1, 1)
  sp_move(1, spr_x(6),  spr_y(28), 79) /* ニャームコ左
